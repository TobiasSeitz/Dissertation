keywords = {Coping Strategies,Password Manager,authentication,graphical passwords,password managers},
mendeley-tags = {Coping Strategies,Password Manager},
pages = {39--52},
publisher = {ACM},
title = {{A Password Manager that Doesn't Remember Passwords}},
year = {2014}
