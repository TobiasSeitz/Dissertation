isbn = {9781450313124},
keywords = {Mental Models,Password Manager,Password managers,Smartphones,Usable security,tr},
mendeley-tags = {Mental Models,Password Manager,tr},
pages = {89--99},
publisher = {ACM Press},
title = {{Tapas: Design, Implementation, and Usability Evaluation of a Password Manager}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84872121864{\&}partnerID=tZOtx3y1 http://dl.acm.org/citation.cfm?doid=2420950.2420964},
year = {2012}
