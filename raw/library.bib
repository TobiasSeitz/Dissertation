journal = {Human Interactive Proofs 2005},
keywords = {Methodology,Phishing,Quantitative},
mendeley-tags = {Methodology,Phishing,Quantitative},
number = {May},
pages = {127--141},
title = {{Phish and hips: Human interactive proofs to detect phishing attacks}},
url = {http://link.springer.com/chapter/10.1007/11427896{\_}9},
volume = {3517},
year = {2005}
}
