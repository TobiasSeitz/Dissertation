keywords = {Coping Strategies,Metrics,Policies,Systems,Technical},
mendeley-tags = {Coping Strategies,Metrics,Policies,Systems,Technical},
pages = {1--10},
publisher = {IEEE},
title = {{Password-based Authentication: A system Perspective}},
year = {2004}
