isbn = {9781450333627},
keywords = {Foundational,Metrics,Privacy,measurement,security behavior,tr,user studies},
mendeley-tags = {Foundational,Metrics,Privacy,tr},
pages = {5257--5261},
publisher = {ACM Press},
title = {{Behavior Ever Follows Intention? A Validation of the Security Behavior Intentions Scale (SeBIS) Serge}},
