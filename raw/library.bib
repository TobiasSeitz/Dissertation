journal = {Animal Behaviour},
keywords = {behaviour,definition,level of organization,philosophy of science},
number = {1},
pages = {103--110},
pmid = {20160973},
title = {{Behavioural biologists do not agree on what constitutes behaviour}},
volume = {78},
year = {2009}
