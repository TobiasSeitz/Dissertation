issn = {1931-2458},
journal = {Communication Methods and Measures},
keywords = {Behavioral Economics,Decision Making,Decisions,Nudge,Persuasion,Psychology},
mendeley-tags = {Behavioral Economics,Decision Making,Decisions,Nudge,Persuasion,Psychology},
number = {March 2015},
pages = {116--137},
title = {{How Can We Tell When a Heuristic Has Been Used? Design and Analysis Strategies for Capturing the Operation of Heuristics}},
