doi = {10.1037/0033-2909.117.1.21},
file = {:Users/Tobi/Library/Application Support/Mendeley Desktop/Downloaded/Gilbert et al. - 1995 - The Correspondence Bias.pdf:pdf},
isbn = {0033-2909 (Print)$\backslash$r0033-2909 (Linking)},
