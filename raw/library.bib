isbn = {9781467350778},
keywords = {data acquisition,interactions,large-scale events,mobility,participatory sensing,spatio-temporal data},
number = {March},
pages = {242--247},
publisher = {IEEE},
title = {{Participatory Bluetooth Sensing: A Method for Acquiring Spatio-temporal Data about Participant Mobility and Interactions at Large Scale Events}},
year = {2013}
