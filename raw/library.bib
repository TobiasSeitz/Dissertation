file = {:Users/Tobi/Library/Application Support/Mendeley Desktop/Downloaded/Bonneau et al. - 2015 - Secrets, Lies, and Account Recovery Lessons from the Use of Personal Knowledge Questions at Google.pdf:pdf},
isbn = {9781450334693},
keywords = {Authentication,Coping Strategies,Privacy,Security,account recovery,com,google,miwilliamson,personal knowledge questions},
mendeley-tags = {Coping Strategies},
pages = {141--150},
publisher = {ACM Press},
title = {{Secrets, Lies, and Account Recovery}},
url = {http://www.www2015.it/documents/proceedings/forms/index.htm http://dl.acm.org/citation.cfm?doid=2736277.2741691},
