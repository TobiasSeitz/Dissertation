isbn = {9781450323970},
keywords = {behavioral,bluetooth,irm,media consumption,modeling,personal data,sensor data},
pages = {11--18},
publisher = {ACM},
title = {{Crowds, Bluetooth, and Rock-n-Roll: Understanding Music Festival Participant Behavior}},
