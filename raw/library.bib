journal = {The Journal of Strategic Information Systems},
keywords = {Business-to-consumer electronic commerce,Electronic commerce,Privacy,Security,Trust,Web shopping},
number = {3-4},
pages = {245--270},
title = {{Trustworthiness in electronic commerce: the role of privacy, security, and site attributes}},
