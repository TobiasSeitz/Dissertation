volume = {53},
year = {2009}
}
@article{Notoatmodjo2009,
abstract = {The security of many computer systems hinges on the secrecy of a single word - if an adversary obtains knowledge of a password, they will gain access to the resources controlled by this password. Human users are the 'weakest link' in password control, due to our propensity to reuse passwords and to create weak ones. Policies which forbid such unsafe password practices are often violated, even if these policies are well-advertised. We have studied how users perceive their accounts and their passwords. Our participants mentally classified their accounts and passwords into a few groups, based on a small number of perceived similarities. Our participants used stronger passwords, and reused passwords less, in account groups which they considered more important. Our participants thus demonstrated awareness of the basic tenets of password safety, but they did not behave safely in all respects. Almost half of our participants reused at least one of the passwords in their high-importance accounts. Our findings add to the body of evidence that a typical computer user suffers from 'password overload'. Our concepts of password and account grouping point the way toward more intuitive user interfaces for password and account-management systems. {\textcopyright} 2009, Australian Computer Society, Inc.},
author = {Notoatmodjo, Gilbert and Thomborson, Clark},
isbn = {9781920682798},
issn = {14451336},
journal = {Conferences in Research and Practice in Information Technology Series},
keywords = {Authentication,Identity management and identity theft,Password practices,Security},
pages = {71--78},
title = {{Passwords and perceptions}},
volume = {98},
year = {2009}
}
@article{Florencio2007,
abstract = {We report the results of a large scale study of password use andpassword re-use habits. The study involved half a million users over athree month period. A client component on users' machines recorded a variety of password strength, usage and frequency metrics. This allows us to measure or estimate such quantities as the average number of passwords and average number of accounts each user has, how many passwords she types per day, how often passwords are shared among sites, and how often they are forgotten. We get extremely detailed data on password strength, the types and lengths of passwords chosen, and how they vary by site. The data is the first large scale study of its kind, and yields numerous other insights into the role the passwords play in users' online experience.},
annote = {1) Welches Problem haben sich die Autoren angesehen?

How passwords are used in browsers and what "password re-use habits" can be found.

2) Was haben die Autoren getan und wie hab sie gemessen?

3 months large scale user study to measure password strength and frequency metrics.

The study analysed the way of how passwords are used, their strength, number of accounts. Also which password type/length is used for which type of website

3) Was war ihr Ziel?

Find out how large the role of web passwords is in users lives.

4) Was haben sie herausgefunden?

The results confirm previous estimations about password weakness and reuse of passwords. In contrast to those estimations the study's resulting dataset allows to analyse users password habits in more statistical and viable ways. 

"The average password appears to be used at about 6 different sites"

Study analysis shows that users certainly have more passwords than they might remember in combination with the correct website and credentials even though most passwords are weak due to higher memorability. The results show that passwords are reused a lot and the average time between logins is more than 5 days (according to the 20 most commonly used login sites in 2007).

Passwords are forgotten a lot:
"For the example of Yahoo, password change operations occurred 15 {\%} as frequently as sign-in operations."

5) Welche L{\"{u}}cken bleiben trotz der Ergebnisse, bzw. welche Kritikpunkte gibt es?

It's a analysis only. There is no suggestion on how to raise awareness to password re-use or login habits. The study provides measured data upon which password management systems can be based in order to take users behavior in account.},
author = {Florencio, Dinei and Herley, Cormac},
doi = {10.1145/1242572.1242661},
isbn = {9781595936547},
issn = {08963207},
journal = {Proceedings of the 16th international conference on World Wide Web - WWW '07},
keywords = {authentication,measurements,password},
pages = {657},
title = {{A large-scale study of web password habits}},
url = {http://dl.acm.org/event.cfm?id=RE334{\%}7B{\&}{\%}7DCFID=416695552{\%}7B{\&}{\%}7DCFTOKEN=47817390{\%}5Cnhttp://portal.acm.org/citation.cfm?doid=1242572.1242661},
