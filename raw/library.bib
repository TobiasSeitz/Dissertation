doi = {10.1145/2508859.2516726},
file = {:Users/Tobi/Library/Application Support/Mendeley Desktop/Downloaded/Mazurek et al. - 2013 - Measuring password guessability for an entire university.pdf:pdf},
isbn = {9781450324779},
issn = {15437221},
keywords = {Field Research,Methodology,Metrics,authentication,classroom use is granted,copies are not made,or,or distributed,or hard copies of,part or all of,password security,passwords,permission to make digital,this work for personal,without fee provided that},
