journal = {SSRN Electronic Journal},
keywords = {Field Research,Personality,Personality traits,Phishing,Security},
mendeley-tags = {Field Research,Personality,Phishing},
title = {{Spear-Phishing in the Wild: A Real-World Study of Personality, Phishing Self-Efficacy and Vulnerability to Spear-Phishing Attacks}},
url = {http://papers.ssrn.com/abstract=2544742{\%}5Cnhttp://dx.doi.org/10.2139/ssrn.2544742{\%}5Cnhttp://www.ssrn.com/abstract=2544742 http://www.ssrn.com/abstract=2544742},
